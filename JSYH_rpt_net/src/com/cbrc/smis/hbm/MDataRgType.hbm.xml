        name="reportIns"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="DATA_RANGE_ID" />
        </key>
        <one-to-many 
            class="com.cbrc.smis.hibernate.ReportIn"
        />
    </set>

