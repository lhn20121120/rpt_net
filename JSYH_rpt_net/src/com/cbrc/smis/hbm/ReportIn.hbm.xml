		</generator>
    </id>

	<property
		name="orgId"
		type="java.lang.String"
		column="ORG_ID"
		length="20"
	/>
    <property
        name="term"
        type="java.lang.Integer"
        column="TERM"
        length="10"
    />
    <property
        name="times"
        type="java.lang.Integer"
        column="TIMES"
        length="10"
    />
    <property
        name="year"
        type="java.lang.Integer"
        column="YEAR"
        length="10"
    />
    <property
        name="tblOuterValidateFlag"
        type="java.lang.Short"
