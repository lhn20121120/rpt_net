        length="5"
    />
    <property
        name="reportDataWarehouseFlag"
        type="java.lang.Short"
        column="REPORT_DATA_WAREHOUSE_FLAG"
        length="5"
    />
    <property
        name="tblInnerValidateFlag"
        type="java.lang.Short"
        column="TBL_INNER_VALIDATE_FLAG"
        length="5"
    />
    <property
        name="repName"
        type="java.lang.String"
        column="REP_NAME"
        length="50"
