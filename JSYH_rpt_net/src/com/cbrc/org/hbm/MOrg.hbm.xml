<class 
    name="com.cbrc.org.hibernate.MOrg" 
    table="M_ORG"
>

    <id
        name="orgId"
        type="java.lang.String"
        column="ORGID"
    >
      <generator class="assigned" />
		
    </id>

    <property
        name="orgName"
        type="java.lang.String"
        column="ORGNAME"
        length="200"
    />
    <property
        name="orgType"
        type="java.lang.String"
        column="ORGTYPE"
        length="6"
    />
    <property
        name="isCorp"
        type="java.lang.String"
        column="ISCORP"
        length="1"
    />
    <property
        name="orgClsId"
        type="java.lang.String"
        column="ORGCLSID"
        length="10"
    />
    <property
        name="orgCode"
        type="java.lang.String"
        column="ORGCODE"
        length="30"
    />

    <!-- Associations -->
  

</class>
</hibernate-mapping>
