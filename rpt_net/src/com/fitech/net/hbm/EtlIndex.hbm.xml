<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
    <class name="com.fitech.net.hibernate.EtlIndex" table="ETL_INDEX">
        <id name="indexName" type="string">
            <column name="INDEX_NAME" length="100" />
            <generator class="assigned" />
        </id>
        <property name="formual" type="string">
            <column name="FORMUAL" length="2000" />
        </property>
        <property name="desc" type="string">
            <column name="DESC" length="20" />
        </property>
    </class>
</hibernate-mapping>
