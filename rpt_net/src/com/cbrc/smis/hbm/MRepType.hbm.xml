<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.cbrc.smis.hibernate.MRepType" 
    table="M_REP_TYPE"
>

    <id
        name="repTypeId"
        type="java.lang.Integer"
        column="REP_TYPE_ID"
    >
        <generator class="sequence">
        	<param name="sequence">seq_m_rep_type</param>
		</generator>
    </id>

    <property
        name="repTypeName"
        type="java.lang.String"
        column="REP_TYPE_NAME"
        length="20"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to MMainRep -->
    <set
        name="MMainReps"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="REP_TYPE_ID" />
        </key>
        <one-to-many 
            class="com.cbrc.smis.hibernate.MMainRep"
        />
    </set>

</class>
</hibernate-mapping>
