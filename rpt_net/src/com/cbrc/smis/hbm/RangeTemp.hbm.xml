<class 
    name="com.cbrc.smis.hibernate.RangeTemp" 
    table="RANGE_TEMP"
>
	<id
        name="orgId" 
        column="ORG_ID" 
        type="java.lang.String"
        length="20"
    >
        <generator class="increment">
		</generator>
    </id>
	
    <property
        name="childRepId" 
        column="CHILD_REP_ID" 
        type="java.lang.String"
        length="5"
    >  
    </property>

	<property
        name="versionId" 
        column="VERSION_ID" 
        type="java.lang.String"
        length="4"
    >  
    </property>
	
	<property
        name="orgClsId"
		type="java.lang.String"
		column="Org_Cls_Id"
		length="10"
    >  
    </property>

</class>
