<class 
    name="com.gather.hibernate.Client" 
    table="CLIENT"
>

    <id
        name="id"
        type="java.lang.String"
        column="ID"
    >
        <generator class="assigned" />
    </id>

    <property
        name="clientVersion"
        type="java.lang.String"
        column="CLIENT_VERSION"
        length="50"
    />
    <property
        name="clientSize"
        type="java.lang.Integer"
        column="CLIENT_SIZE"
        length="22"
    />
    <property
        name="note"
        type="java.lang.String"
        column="NOTE"
        length="100"
    />
    <property
        name="clientEntity"
        type="java.lang.String"
        column="CLIENT_ENTITY"
        length="20"
    />
    <property
        name="downTime"
        type="java.util.Date"
        column="DOWN_TIME"
        length="7"
    />

    <!-- Associations -->
  

</class>
</hibernate-mapping>
