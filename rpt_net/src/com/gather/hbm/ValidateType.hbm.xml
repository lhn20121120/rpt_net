<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
<!-- 
    Created by the Middlegen Hibernate plugin 2.1

    http://boss.bekk.no/boss/middlegen/
    http://www.hibernate.org/
-->

<class 
    name="com.gather.hibernate.ValidateType" 
    table="VALIDATE_TYPE"
>

    <id
        name="validateTypeId"
        type="java.lang.Integer"
        column="VALIDATE_TYPE_ID"
    >
        <generator class="assigned" />
    </id>

    <property
        name="validateTypeName"
        type="java.lang.String"
        column="VALIDATE_TYPE_NAME"
        length="20"
    />

    <!-- Associations -->
  
    <!-- bi-directional one-to-many association to DataValidateInfo -->
    <set
        name="dataValidateInfos"
        lazy="true"
        inverse="true"
		cascade="none"
    >
        <key>
            <column name="VALIDATE_TYPE_ID" />
        </key>
        <one-to-many 
            class="com.gather.hibernate.DataValidateInfo"
        />
    </set>

</class>
</hibernate-mapping>
